{
  "account": {
    "description": "CI Runner",
    "username": "ci-runner",
    "password": "ci-runner"
  },
  "image": {
    "name": "ci",
    "cpu": 4,
    "ram": 8,
    "disk": 100,
    "version": "sequoia"
  },
  "install": {
    "brew": {
      "casks": [
        "git-credential-manager",
        "iterm2",
        "tuist"
      ],
      "packages": [
        "awscli",
        "ca-certificates",
        "cmake",
        "cocoapods",
        "curl",
        "fastlane",
        "gh",
        "gitlab-runner",
        "glfw",
        "jfrog-cli",
        "jq",
        "mc",
        "ninja",
        "python@3.11",
        "swiftly",
        "unzip",
        "wget",
        "yq",
        "zip"
      ]
    },
    "swift": "6.1.2",
    "xcode": [
      { "version": "16.2.0", "select": true }
    ],
    "xcode-platforms": [
      { "platform": "iOS", "version": "15.5" },
      { "platform": "iOS", "version": "16.4" },
      { "platform": "iOS", "version": "17.5" },
      { "platform": "iOS", "version": "18.4" }
    ],
    "vulkan-sdk": "1.4.313.1"
  }
}
